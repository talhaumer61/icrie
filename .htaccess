RewriteEngine on

RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)$ index.php?control=$1&zone=$2 [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/$ index.php?control=$1&zone=$2 [QSA,L]

RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/page/(.*)/$ index.php?control=$1&zone=$2&page=$3 [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/page/(.*)$ index.php?control=$1&zone=$2&page=$3 [QSA,L]

RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/page/(.*)/$ index.php?control=$1&zone=$2&category=$3 [QSA,L] [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/page/(.*)$ index.php?control=$1&zone=$2&category=$3 [QSA,L]

RewriteRule ^([a-zA-Z0-9_-]+)/page/(.*)/ index.php?control=$1&page=$2 [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/page/(.*) index.php?control=$1&page=$2 [QSA,L]

# RewriteRule ^([a-zA-Z0-9_-]+)/wrds/([a-zA-Z0-9_-]+)/page/(.*)/ index.php?control=$1&wrds=$2&page=$3 [QSA,L]
# RewriteRule ^([a-zA-Z0-9_-]+)/wrds/([a-zA-Z0-9_-]+)/page/(.*) index.php?control=$1&wrds=$2&page=$3 [QSA,L]

RewriteRule ^([a-zA-Z0-9_-]+)$ index.php?control=$1 [QSA,L]
RewriteRule ^([a-zA-Z0-9_-]+)/$ index.php?control=$1 [QSA,L]


RewriteRule ^(.*)\.html$ $1.php
RewriteRule ^index index.php

# php -- BEGIN cPanel-generated handler, do not edit
